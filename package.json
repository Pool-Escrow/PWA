{
  "name": "pool-pwa",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.2.19",
  "engines": {
    "node": "22.17.0"
  },
  "scripts": {
    "analyze": "ANALYZE=true bun run build:next",
    "build": "bun run generate:icons && next build",
    "build:next": "bun run generate:icons && next build",
    "clean": "rm -rf .next .turbo node_modules",
    "clean:reinstall": "rm -rf node_modules .next bun.lock && bun install",
    "dev": "turbo dev:next",
    "dev:next": "next dev --turbopack",
    "fmt:all": "bun lint:fix && bun fmt:pkg && bun typecheck",
    "fmt:check": "eslint",
    "fmt:code": "eslint --fix",
    "fmt:pkg": "bunx sort-package-json",
    "generate:icons": "bun scripts/generate-icon-assets.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "unused": "knip"
  },
  "dependencies": {
    "@number-flow/react": "0.5.10",
    "@privy-io/react-auth": "2.21.1",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-slot": "1.2.3",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-query": "5.84.1",
    "blo": "2.0.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "hono": "4.8.12",
    "lucide-react": "0.536.0",
    "motion": "12.23.12",
    "next": "15.4.5",
    "next-view-transitions": "0.3.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "tailwindcss-safe-area": "1.1.0",
    "viem": "2.33.2",
    "zod": "4.0.15",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.1.0",
    "@eslint-react/eslint-plugin": "1.52.3",
    "@iconify/json": "2.2.366",
    "@iconify/utils": "3.0.0",
    "@next/bundle-analyzer": "15.4.5",
    "@next/eslint-plugin-next": "15.4.5",
    "@prettier/plugin-xml": "3.4.2",
    "@tailwindcss/postcss": "4.1.11",
    "@tanstack/eslint-plugin-query": "5.83.1",
    "@types/node": "22.17.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "eslint-plugin-better-tailwindcss": "3.7.4",
    "eslint-plugin-format": "1.0.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "glob": "11.0.3",
    "knip": "5.62.0",
    "tailwindcss": "4.1.11",
    "turbo": "2.5.5",
    "tw-animate-css": "1.3.6",
    "typescript": "5.8.3"
  },
  "overrides": {},
  "patchedDependencies": {
    "@antfu/eslint-config@5.1.0": "config/patches/@antfu%2Feslint-config@5.1.0.patch"
