{
    "$schema": "https://json.schemastore.org/package",
    "name": "pool-pwa",
    "version": "0.0.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "next build",
        "check-types": "tsc --noemit",
        "dev": "next dev",
        "gen": "node scripts/generate-types.mjs",
        "gen:contracts": "pnpm gen contracts",
        "gen:css": "pnpm gen css",
        "gen:db": "pnpm gen db",
        "lint": "next lint",
        "preinstall": "npx only-allow pnpm",
        "prettify": "prettier -w",
        "start": "next start",
        "test": "vitest"
    },
    "dependencies": {
        "@biconomy/account": "4.4.6",
        "@privy-io/react-auth": "1.70.0",
        "@privy-io/server-auth": "1.7.3",
        "@radix-ui/react-alert-dialog": "1.0.5",
        "@radix-ui/react-avatar": "1.0.4",
        "@radix-ui/react-dialog": "1.0.5",
        "@radix-ui/react-dropdown-menu": "2.0.6",
        "@radix-ui/react-label": "2.0.2",
        "@radix-ui/react-popover": "1.0.7",
        "@radix-ui/react-progress": "1.0.3",
        "@radix-ui/react-select": "2.0.0",
        "@radix-ui/react-slot": "1.0.2",
        "@radix-ui/react-tabs": "1.0.4",
        "@radix-ui/react-toast": "1.1.5",
        "@serwist/next": "9.0.3",
        "@supabase/ssr": "0.3.0",
        "@supabase/supabase-js": "2.43.4",
        "@tanstack/query-sync-storage-persister": "5.45.0",
        "@tanstack/react-query": "5.45.0",
        "@tanstack/react-query-persist-client": "5.45.0",
        "@wagmi/core": "2.11.2",
        "axios": "1.7.2",
        "class-variance-authority": "0.7.0",
        "clsx": "2.1.1",
        "cmdk": "1.0.0",
        "date-fns": "3.6.0",
        "ethers": "6.13.0",
        "framer-motion": "11.2.10",
        "js-cookie": "3.0.5",
        "jsonwebtoken": "9.0.2",
        "lodash": "4.17.21",
        "lucide-react": "0.394.0",
        "next": "14.2.4",
        "next-themes": "0.3.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-qr-code": "2.0.14",
        "react-qr-reader": "3.0.0-beta-1",
        "sharp": "0.33.4",
        "tailwind-merge": "2.3.0",
        "tailwindcss-animate": "1.0.7",
        "vaul": "0.9.1",
        "viem": "2.13.10",
        "wagmi": "2.10.2"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "14.2.4",
        "@tanstack/eslint-plugin-query": "5.43.1",
        "@tanstack/react-query-devtools": "5.45.0",
        "@testing-library/jest-dom": "6.4.6",
        "@testing-library/react": "16.0.0",
        "@testing-library/user-event": "14.5.2",
        "@types/js-cookie": "3.0.6",
        "@types/jsonwebtoken": "9.0.6",
        "@types/lodash": "4.17.5",
        "@types/node": "20.14.2",
        "@types/react": "18.3.3",
        "@typescript-eslint/eslint-plugin": "7.13.1",
        "@vitejs/plugin-react": "4.3.1",
        "@wagmi/cli": "2.1.9",
        "autoprefixer": "10.4.19",
        "eslint": "8.57.0",
        "eslint-config-next": "14.2.4",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-tailwindcss": "3.17.3",
        "jsdom": "24.1.0",
        "postcss": "8.4.38",
        "prettier": "3.3.2",
        "prettier-plugin-tailwindcss": "0.6.4",
        "tailwindcss": "3.4.4",
        "tailwindcss-classnames": "3.1.0",
        "tailwindcss-safe-area": "0.5.1",
        "typescript": "5.4.5",
        "vite-tsconfig-paths": "4.3.2",
        "vitest": "1.6.0"
    },
    "pnpm": {
        "overrides": {
            "react": "18.3.1",
            "react-dom": "18.3.1",
            "i18next": "23.2.3",
            "rimraf": "5.0.7"
        },
        "allowedDeprecatedVersions": {
            "@babel/plugin-proposal-async-generator-functions": "7.20.7",
            "@babel/plugin-proposal-class-properties": "7.18.6",
            "@babel/plugin-proposal-logical-assignment-operators": "7.20.7",
            "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
            "@babel/plugin-proposal-numeric-separator": "7.18.6",
            "@babel/plugin-proposal-object-rest-spread": "7.20.7",
            "@babel/plugin-proposal-optional-catch-binding": "7.18.6",
            "@babel/plugin-proposal-optional-chaining": "7.21.0",
            "@humanwhocodes/config-array": "0.11.14",
            "@humanwhocodes/object-schema": "2.0.3",
            "@motionone/vue": "10.16.4",
            "glob": "7.2.3",
            "inflight": "1.0.6",
            "querystring": "0.2.1",
            "rollup-plugin-terser": "7.0.2",
            "sourcemap-codec": "1.4.8",
            "workbox-cacheable-response": "6.6.0",
            "workbox-google-analytics": "6.6.0"
        }
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "endOfLine": "lf",
        "jsxSingleQuote": true,
        "printWidth": 120,
        "proseWrap": "preserve",
        "quoteProps": "consistent",
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all",
        "useTabs": false,
        "tabWidth": 4
    },
    "eslintConfig": {
        "parserOptions": {
            "project": "./tsconfig.eslint.json"
        },
        "extends": [
            "next/core-web-vitals",
            "plugin:@typescript-eslint/recommended-type-checked",
            "plugin:@tanstack/eslint-plugin-query/recommended",
            "plugin:tailwindcss/recommended",
            "prettier"
        ],
        "rules": {
            "react/self-closing-comp": 1,
            "@typescript-eslint/no-unused-vars": [
                1,
                {
                    "argsIgnorePattern": "^_",
                    "varsIgnorePattern": "^_"
                }
            ],
            "@typescript-eslint/consistent-type-imports": 1
        }
    }
}
